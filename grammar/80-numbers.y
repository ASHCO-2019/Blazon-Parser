
/* --------------------------------- Numbers -------------------------------- */

twoOrMore:
    NUMBER { $$ = $1; }
    | TWO { $$ = $1; }
    | THREE { $$ = $1; }
    | FOUR { $$ = $1; }
    | FIVE { $$ = $1; }
    | SIX { $$ = $1; }
    ;

number:
    twoOrMore
    | ONE  { $$ = $1; }
    | THE { $$ = "1"; }
    ;

